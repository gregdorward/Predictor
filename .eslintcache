ableCell align=\"center\">{`${props.BTTSForecast}%`}</StyledTableCell>\n            <StyledTableCell align=\"center\">{props.BTTSOdds}</StyledTableCell>\n          </StyledTableRow>\n\n          <StyledTableRow key=\"Corners\">\n            <StyledTableCell component=\"th\" scope=\"row\" align=\"center\">\n              Over 10.5 Corners\n            </StyledTableCell>\n            <StyledTableCell component=\"th\" scope=\"row\" align=\"center\">\n              {`${props.CornersStat}`}\n            </StyledTableCell>\n            <StyledTableCell align=\"center\">{corners}</StyledTableCell>\n            <StyledTableCell align=\"center\">{props.CornersOdds}</StyledTableCell>\n          </StyledTableRow>\n        </TableBody>\n      </Table>\n    </TableContainer>\n  );\n}\n",{"ruleId":"145","replacedBy":"146"},{"ruleId":"147","replacedBy":"148"},{"ruleId":"149","severity":1,"message":"150","line":10,"column":3,"nodeType":"151","messageId":"152","endLine":10,"endColumn":8},{"ruleId":"149","severity":1,"message":"153","line":12,"column":3,"nodeType":"151","messageId":"152","endLine":12,"endColumn":24},{"ruleId":"149","severity":1,"message":"154","line":13,"column":3,"nodeType":"151","messageId":"152","endLine":13,"endColumn":11},{"ruleId":"149","severity":1,"message":"155","line":14,"column":3,"nodeType":"151","messageId":"152","endLine":14,"endColumn":12},{"ruleId":"149","severity":1,"message":"156","line":3,"column":26,"nodeType":"151","messageId":"152","endLine":3,"endColumn":34},{"ruleId":"149","severity":1,"message":"157","line":412,"column":11,"nodeType":"151","messageId":"152","endLine":412,"endColumn":26},{"ruleId":"149","severity":1,"message":"158","line":413,"column":11,"nodeType":"151","messageId":"152","endLine":413,"endColumn":26},{"ruleId":"149","severity":1,"message":"159","line":452,"column":11,"nodeType":"151","messageId":"152","endLine":452,"endColumn":26},{"ruleId":"149","severity":1,"message":"160","line":453,"column":11,"nodeType":"151","messageId"